{{#partial "htmlTitle"}}{{service.name}}{{/partial}}

{{#partial "content"}}

<h1>{{service.name}}</h1>

{{{service.descriptionMarkup}}}
  

{{#if service.linkRelations}}

<hr/>

<h2>Link relations</h2>
<table class="table">
  <thead>
    <tr>
      <th>Link Relation</th>
      <th>Description</th>
      <th>Docs</th>
    </tr>
  </thead>
  <tbody>
    {{#each service.linkRelations}}
    <tr>
      <td><a href="{{docsContext.baseUrl}}{{this.rel}}"><strong>{{this.rel}}</strong></a></td>
      <td>{{{this.descriptionMarkup}}}</td>
      <td><a href="{{docsContext.baseUrl}}{{this.rel}}" title="Documentation"><i class="icon-book"></i></a></td>
    </tr>
    {{/each}}
  </tbody>
</table>

{{/if}}


{{/partial}}

{{> base.html.hbs}}
